<% unless section.section_setting.sections.count == 1 %>
	<div class="sec-comb">
		<div>Course Sections with Same Settings</div>
	</div>
	<% section.section_setting.sections.each do |sec| %>
		<% unless section == sec %>
			<%= render 'sec_row', name: "#{sec.course_id_s}", value: "#{sec.course.name}" %>
		<% end %>
	<% end %>
<% end %>